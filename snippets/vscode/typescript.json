{
  "Add Debugger": {
    "prefix": "d",
    "body": "debugger"
  },
  "Add console.log()": {
    "prefix": "cl",
    "body": "console.log(${0})"
  },
  "Add console.log() with arrow": {
    "prefix": "ca",
    "body": "console.log('--> ${1}:', ${0})"
  },
  "Add console.dir()": {
    "prefix": "cd",
    "body": "console.dir(${0})"
  },
  "Add console.table()": {
    "prefix": "ct",
    "body": "console.table(${0})"
  }
}
