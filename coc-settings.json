{
  // https://github.com/neoclide/coc.nvim/blob/master/data/schema.json
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.displayByAle": false,
  "diagnostic.messageTarget": "float",
  "diagnostic.maxWindowHeight": 20,
