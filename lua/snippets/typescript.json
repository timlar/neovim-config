{
  "debugger": {
    "prefix": "dd",
    "body": "debugger"
  },
  "console.log": {
    "prefix": "cl",
    "body": "console.log(${0})"
  },
  "console.log a variable": {
    "prefix": "cv",
    "body": "console.log('--> ${1}:', ${0})"
  },
  "console.dir": {
    "prefix": "cd",
    "body": "console.dir(${0})"
  },
  "console.table": {
    "prefix": "ct",
    "body": "console.table(${0})"
  }
}
